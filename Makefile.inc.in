LIBBTBBLIBS = @LIBBTBBLIBS@
LIBBTBBCFLAGS = @LIBBTBBCFLAGS@

LIBUSBLIBS = @LIBUSBLIBS@
LIBUSBCFLAGS = @LIBUSBCFLAGS@

SUIDGROUP 	= @suidgroup@

DATASOURCE_LIBS	+= $(CAPLIBS) @PTHREAD_LIBS@ -lm -lrabbitmq

prefix 		= @prefix@
exec_prefix = @exec_prefix@
